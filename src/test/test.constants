test()
{
  auto x,y;
  print "10pm1+10pm1 = ",10pm1+10pm1,"\n";
  print "2*10pm1    = ",2*10pm1,"\n";
  x=10pm1;
  y:=x;
  print "x=",x," y:=x = ",y,"\n";
  print "x+x=",x+x,"\n";
  print "x+y=",x+y,"\n";;
}
f(){return 10pm1;}
g(){return 10pm1;}
test();
print "f()*g() = ",f()*g(),"\n";
